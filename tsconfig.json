{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "isolatedModules" : true,
    "useDefineForClassFields": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}
